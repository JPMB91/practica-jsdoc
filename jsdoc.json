{
    "plugins": ["jsdoc-http-plugin"],
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs)"
    },
    "templates": {
        "cleverlinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": true,
        "destination": "./docs",
        "template": "node_modules/docdash",
        "tutorials": "./tutorials",
        "readme": "./README.md"
    }
}